{
	"name": "RaiPlaySoundRSS",
	"image": "node:20-bullseye",
	"customizations": {
		"vscode": {
			"settings": {},
			"extensions": [
				"dbaeumer.vscode-eslint",
				"ms-azuretools.vscode-docker"
			]
		}
	},
	"forwardPorts": [
		3000
	],
	"mounts": [
		"source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind"
	],
	"remoteUser": "node",
	"features": {
		"common": {
			"username": "node",
			"uid": "automatic",
			"gid": "automatic",
			"installZsh": true,
			"installOhMyZsh": true,
			"upgradePackages": true,
			"nonFreePackages": false
		},
		"docker-from-docker": {
			"version": "latest",
			"moby": true
		}
	}
}